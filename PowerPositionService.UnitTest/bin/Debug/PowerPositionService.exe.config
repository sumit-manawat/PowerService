<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>


  <appSettings>

    <add key="ServiceName" value="PowerPositonService" />
    
    <!--Power Position Interval in Minutes-->
    <add key="PowerPositionInterval" value="1"/>
    <add key="PowerPositionDateFormat" value="HH:mm"/>
    <add key="FileLocation" value="C:\PowerPositions"/>
    <add key="FilePrefix" value="PowerPosition_"/>
    <add key="FileSuffixFormat" value="yyyyMMdd_HHmm"/>

  </appSettings>
</configuration>